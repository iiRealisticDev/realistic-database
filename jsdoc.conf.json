{
  "plugins": ["plugins/markdown", "plugins/summarize"],
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./dist/modules", "./README.md"]
  },
  "opts": {
    "template": "./node_modules/clean-jsdoc-theme",
    "encoding": "utf8",
    "prism-theme": "prism-custom",
    "destination": "../iiRealisticDev.github.io/realistic-database",
    "recurse": true,
    "verbose": true,
    "theme_opts": {
      "theme": "dark",
      "meta": [
        {
          "name": "Author",
          "content": "iiRealistic_Dev"
        },
        {
          "name": "Description",
          "value": "A simple JSON-based database."
        }
      ],
      "menu": [
        {
          "title": "GitHub Repo",
          "id": "github",
          "link": "https://github.com/iiRealisticDev/realistic-database"
        }
      ]
    }
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
