{
  "plugins": ["plugins/markdown", "plugins/summarize"],
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./dist/modules", "./README.md"]
  },
  "opts": {
    "template": "node_modules/better-docs",
    "encoding": "utf8",
    "destination": "../iiRealisticDev.github.io/realistic-database",
    "recurse": true,
    "verbose": true,
    "tutorials": "./tutorials"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "better-docs": {
    "name": "Realistic Database",
    "logo": "./logo.png",
    "title": "Realistic Database",
    "navLinks": [
      {
        "label": "GitHub Repo",
        "href": "https://github.com/iiRealisticDev/realistic-database"
      },
      {
        "label": "NPM Page",
        "href": "https://www.npmjs.com/package/realistic-database"
      }
    ]
  }
}
